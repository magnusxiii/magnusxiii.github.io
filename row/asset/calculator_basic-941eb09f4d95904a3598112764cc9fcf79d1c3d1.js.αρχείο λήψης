or(let c of s.den.args)if(!vT(c)){if(c.type!==\"Digit\")return;a.push(c.val)}let u=Jr(n,s.span);return Or(t,FE(u,i.join(\"\"),o.join(\"\"),a.join(\"\")))}function vT(e){switch(e.type){case\"Sqrt\":case\"Frac\":case\"SupSub\":case\"LeftRight\":case\"OperatorName\":case\"TokenNode\":case\"Symbol\":case\"Letter\":case\"Digit\":return!1;case\"Cmd\":return e.val===\"\\\\space\";case\"EscapedSymbol\":return e.val===\"\\\\ \"||e.val===\"\\\\:\"||e.val===\"\\\\,\"||e.val===\"\\\\;\";default:throw`Unexpected atom ${e.type}.`}}function pl(e,t){for(;t<e.length&&vT(e[t]);)t+=1;return t}function wd(e){return Ld(e,\".\")}function Ld(e,t){return e.type===\"Symbol\"&&e.val===t}function Cu(e){return{type:\"initial\",tokenType:e}}function fl(e){return{type:\"l\",tokenType:e}}function dl(e){return{type:\"r\",tokenType:e}}function dt(e){return{type:\"la\",tokenType:e}}function Lu(e){return{type:\"ra\",tokenType:e}}function Pv(e,t){for(let r of gv)if(t[r]===void 0)throw new Error(`Programming Error: token ${r} must be a assigned a ${e} precedence`)}function Va(e,t,r,n){if(t[r]!==void 0)throw new Error(`Programming Error: duplicate ${e} entry for token ${r}.`);t[r]=n}function Iv(e){let t={},r={},n={};for(let a=0;a<e.length;a++){let u=e[a];for(let{type:c,tokenType:l}of u)switch(c){case\"initial\":Va(\"initial\",t,l,a);break;case\"l\":Va(\"left\",r,l,a);break;case\"r\":Va(\"right\",n,l,a);break;case\"la\":Va(\"left\",r,l,a),Va(\"right\",n,l,a);break;case\"ra\":Va(\"left\",r,l,a),Va(\"right\",n,l,a-1);break}}Pv(\"left\",r),Pv(\"right\",n);function i(a){return n[a]}function s(a){return r[a]}function o(a){let u=t[a];return u===void 0?i(a):u}return{rightPrec:i,leftPrec:s,initialPrec:o}}var wu={},SL=[\"sin\",\"cos\",\"tan\",\"cot\",\"sec\",\"csc\",\"sinh\",\"cosh\",\"tanh\",\"coth\",\"sech\",\"csch\"];SL.forEach(function(e){wu[e]=\"arc\"+e,wu[\"arc\"+e]=e});var NL=0;function Ov(e,t){return RL(e,t)}function Ve(e,t){let r=[];for(let n=0;n<t.length;n++)r.push(Gt(e,t[n]));return r}function RL(e,t){return e.setInput(AL(e,t),t.span)}function Dv(e){if(e.type!==\"Call\")return;let[t,r]=e.args,n=Ur(r);if(qr(t)&&n.every(qr))return{base:t,args:n}}function Sv(e){return Dv(e)!==void 0}function Nv(e){return e.type===\"Paren\"&&e.args[0].type===\"Seq\"&&e.args[0].args.every(qr)}function RT(e){return qr(e)||Nv(e)}function AL(e,t){let r=e.nodes;switch(t.type){case\"Equals\":{let[n,i]=t.args,s=Dv(n);if(s){let{base:o,args:a}=s;return r.FunctionDefinition(Gt(e,o),Ve(e,a),Gt(e,i))}else if(RT(n)){let o=Gt(e,i);return Nv(n)?r.CoordinateEquation(Gt(e,n),o):r.Assignment(Gt(e,n),o)}return r.Equation(Gt(e,n),Gt(e,i))}case\"Tilde\":{let[n,i]=Ve(e,t.args);return r.Regression(n,i)}case\"Inequality\":return Ba(e,t);case\"InequalityChain\":{if(t.chain.length>1)throw jp();let{first:{symbol:n,args:[i,s]},chain:[{symbol:o,args:[a]}]}=t;return qr(s)?r.DoubleInequality([Gt(e,i),n,Gt(e,s),o,Gt(e,a)]):Ba(e,t)}case\"Call\":{let[n,i]=t.args;if(n.type===\"Cmd\")switch(n.val){case\"histogram\":return r.Histogram(Ve(e,Ur(i)));case\"dotplot\":return r.DotPlot(Ve(e,Ur(i)));case\"boxplot\":return r.BoxPlot(Ve(e,Ur(i)));case\"ttest\":return r.TTest(Ve(e,Ur(i)));case\"ittest\":return r.IndependentTTest(Ve(e,Ur(i)));case\"stats\":return r.Stats(Ve(e,Ur(i)));default:return ml(e,t)}return ml(e,t)}default:return ml(e,t)}}function Rv(e){if(e.type!==\"Paren\")return!1;let[t]=e.args;return t.type!==\"Seq\"?!1:t.args.length===2||t.args.length===3}function Gt(e,t){return e.setInput(ml(e,t),t.span)}function ml(e,t){let r=e.nodes;switch(t.type){case\"Pos\":return Gt(e,t.args[0]);case\"Neg\":let n=-1,i=t.args[0];for(;;)if(i.type===\"Pos\"||i.type===\"Paren\"&&!Rv(i))i=i.args[0];else if(i.type===\"Neg\")i=i.args[0],n*=-1;else break;switch(i.type){case\"Decimal\":{let o=Ev(i);return r.Constant(n===-1?ai(o):o)}case\"MixedNumber\":{let o=vv(i);return r.MixedNumber(n===-1?ai(o):o)}default:return n===-1?r.Negative([Gt(e,i)]):Gt(e,i)}case\"Add\":return r.Add(Ve(e,t.args));case\"Sub\":return r.Subtract(Ve(e,t.args));case\"